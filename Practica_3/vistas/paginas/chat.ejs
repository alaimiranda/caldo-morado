<h2>Mis chats</h2>
<div id="chat-list">
    <% if (chats.length > 0) { %>
        <% chats.forEach(chat => { %>
            <div class="chat-item id">
                <% if (chat.username_1 === sessionUsername) { %>
                    <strong><%= chat.username_2 %></strong>
                <% } else if (chat.username_2 === sessionUsername) { %>
                    <strong><%= chat.username_1 %></strong>
                <% } %>
            </div>
        <% }) %>
    <% } else { %>
        <p>No hay chats disponibles</p>
    <% } %>
</div>